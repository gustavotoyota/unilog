{
  "name": "unilogr",
  "description": "Modern logger for both Node.js and Browser.",
  "version": "0.0.18",
  "author": "Gustavo Toyota",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "^8.0.1",
    "jest": "29.2.2",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "tsc-alias": "^1.7.0",
    "typescript": "4.8.4",
    "unbuild": "^0.9.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "unilogr",
    "log",
    "logs",
    "logger",
    "logging",
    "debug",
    "universal",
    "node",
    "browser",
    "winston",
    "loglevel"
  ],
  "license": "ISC",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "repository": "https://github.com/gustavotoyota/unilogr.git",
  "scripts": {
    "build": "unbuild",
    "clean": "rimraf ./dist *.tsbuildinfo",
    "fix": "eslint --fix --ext .ts ./",
    "npkill": "rimraf ./node_modules",
    "prepare": "husky install",
    "release": "unbuild && standard-version && git push --follow-tags origin dev && npm publish --access public"
  }
}
